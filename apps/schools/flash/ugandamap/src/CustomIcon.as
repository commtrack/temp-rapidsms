package{	import flash.display.Sprite;	import flash.events.*;		public class CustomIcon extends Sprite{		//private variables		private var iconColor:uint;		private var radius:Number;		private var acolor:Number;				//private var detailsVisible:Boolean = false;				static function unrankedIcon():CustomIcon {			var color:uint = 0xF0F000;			return new CustomIcon(color,1.0);		}				static function rankedIcon(c:Number):CustomIcon {						var blue:uint = 250 * (c/100.0);			var red:uint = 250 - blue;			var green:uint = 40;						var a:Number = Math.abs(c - 50)/100.0 + 0.5; 						var color:uint = red<<16 | green<<8 | blue;			return new CustomIcon(color,a);		}				//constructor		public function CustomIcon(color:uint, a:Number) {	//,r:Number){			this.iconColor = color;					this.acolor = a;			drawIcon();		}				private function drawIcon():void {			graphics.beginFill(iconColor,acolor);			graphics.drawCircle(0,0,5); //(x, y, radius)			graphics.endFill();		}			}}